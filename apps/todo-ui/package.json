{
  "name": "todo-ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "npm-run-all --parallel test:unit test:acceptance",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:unit:watch": "vitest watch --config vitest.config.ts",
    "test:acceptance": "vitest run --config vitest.acceptance.config.ts",
    "test:acceptance:watch": "vitest watch --config vitest.acceptance.config.ts",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.27.0",
    "@demo-todo/api-contracts": "*",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@tanstack/react-query": "^5.62.11",
    "@ts-rest/react-query": "3.53.0-rc.1",
    "framer-motion": "^12.23.22",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^24.0.0",
    "msw": "^2.11.3",
    "vite": "^6.0.11"
  }
}
